text = input("Enter a text: ")
vowels = "aeiouAEIOU"
count = 0

for char in text:
    if char in vowels:
        count += 1

print("Number of vowels:", count)





numbers = [5, 3, 8, 2, 9, 4]
max_number = numbers[0]
for num in numbers:
    if num > max_number:
        max_number = num

print("Maximum number:", max_number)